package constructor;

public class PersonSelf {
	String name;
	float height;
	float weight;
	
	public PersonSelf() {} // 자바 컴파일러가 자동으로 제공하는 디폴드 생성자. 생성자가 하나도 없는 경우에만 제공.
	
	public PersonSelf(String pname) {
		name = pname;
	}
	
	public PersonSelf(String pname,float pheight, float pweight) {
		name = pname;
		height = pheight;
		weight = pweight;
	}
	
}
